{
  "name": "generator-cypress-scaffolding",
  "version": "1.0.0",
  "description": "Scaffolding cypress project",
  "homepage": "https://github.com/PabloGarciaQATesting/cypress-scaffolding",
  "author": {
    "name": "Pablo Alves Garcia",
    "email": "pabloalvesgarcia@gmail.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Cypress",
    "Scaffold",
    "QA",
    "Test",
    "yeoman-generator"
  ],
  "devDependencies": {
    "cypress": "^13.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^7.4.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^5.9.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PabloGarciaQATesting/cypress-scaffolding.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/PabloGarciaQATesting/cypress-scaffolding/issues"
  }
}
